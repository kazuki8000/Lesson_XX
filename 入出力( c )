#include <stdio.h>

int main(int argc, char **argv)
{
	 int cont = 0 ;
    
    do {
        
        int no ;
        
        printf("数値入力");
        scanf("%d" , &no);
        
        if ( no % 10)
            puts("10で割れません");
        else
            puts("10で割れます");
            
        printf("もう一回？　【Yes・・9 / No・・0】 : \n");
        scanf("%d" , &cont);
    } while(cont == 9);
	
	return 0;
}
